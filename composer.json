{
    "name": "badbreze/yii2-attachments",
    "description": "Extension for file uploading and attaching to the models",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","attachment","file","upload"],
    "license": "BSD-4-Clause",
    "homepage": "http://www.divenock.com/projects/yii2-attachments",
    "authors": [
        {
            "name": "Alimzhan Abuov aka Nemmo",
            "email": "alimzhan.abu@yandex.kz"
        },
        {
            "name": "Alexander Palchikov aka AxelPAL",
            "email": "axelpal@gmail.com"
        },
        {
            "name": "Damian Gomez aka Badbreze",
            "email": "info@divenock.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
      "yiisoft/yii2": "~2.0.0",
        "badbreze/yii2-widget-fileinput": ">=2.0",
        "himiklab/yii2-colorbox-widget": "*",
        "yiisoft/yii2-jui": "^2.0",
        "yurkinx/yii2-image": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/dbunit": "~1.0"
    },
    "repositories":
    [
        {
            "type": "vcs",
            "url": "https://github.com/pshirmanov/yii2-widget-fileinput.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "file\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
